void
run_ispell_in_child (pipe_t * the_pipe)
{

  close (the_pipe->pin);
  close (the_pipe->pout);
  close (the_pipe->perr);

  if (the_pipe->cin != 0)
    if (dup2 (the_pipe->cin, 0) != 0)
      error (1, (*__errno_location ()), "error duping to stdin");

  if (the_pipe->cout != 1)
    if (dup2 (the_pipe->cout, 1) != 1)
      error (1, (*__errno_location ()), "error duping to stdout");

  if (the_pipe->cerr != 2)
    if (dup2 (the_pipe->cerr, 2) != 2)
      error (1, (*__errno_location ()), "error duping to stderr");

  if (dictionary != ((void *)0))
    if (execl (ispell_prog, "ispell", "-a", "-p", dictionary, ((void *)0))
 < 0)
      error (1, (*__errno_location ()), "error executing %s", ispell_prog);

  if (british)
    if (execl (ispell_prog, "ispell", "-a", "-d", "british", ((void *)0))
 < 0)
      error (1, (*__errno_location ()), "error executing %s", ispell_prog);

  if (execl (ispell_prog, "ispell", "-a", ((void *)0)) < 0)
    error (1, (*__errno_location ()), "error executing %s", ispell_prog);
}
